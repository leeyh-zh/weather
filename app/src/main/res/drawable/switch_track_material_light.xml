<selector>
    <item xmlns:android="http://schemas.android.com/apk/res/android" android:state_enabled="false">
        <nine-patch android:src="@drawable/op_switch_track_mtrl_alpha" android:tint="@color/switch_track_disable_color_light" />
    </item>
    <item xmlns:android="http://schemas.android.com/apk/res/android" android:state_checked="true">
